{
  "name": "BasicBudgets",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "test": "npm run eslint src/ && jest",
    "eslint": "eslint"
  },
  "jest": {
    "scriptPreprocessor": "./jest/scriptPreprocessor",
    "setupEnvScriptFile": "./jest/jestEnvironment",
    "testFileExtensions": [
      "es6",
      "js"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "es6"
    ],
    "testPathIgnorePatterns": [
      "node_modules/"
    ],
    "unmockedModulePathPatterns": [
      "promise",
      "source-map",
      "key-mirror",
      "immutable",
      "fetch",
      "react-addons-test-utils",
      "react",
      "fbjs",
      "core-js",
      "babel-polyfill"
    ],
    "verbose": true,
    "preprocessCachingDisabled": true,
    "collectCoverage": true
  },
  "dependencies": {
    "alt": "=0.18.1",
    "alt-container": "https://github.com/maximilianhurl/container/tarball/fix-react-native-container",
    "react-native": "^0.16.0"
  },
  "devDependencies": {
    "babel-cli": "=6.4.0",
    "babel-core": "=6.4.0",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "=6.3.13",
    "babel-preset-react": "=6.3.13",
    "eslint": "=1.5.1",
    "eslint-plugin-react": "=3.5.0",
    "jest-cli": "^0.8.2",
    "react": "=0.14.3",
    "react-addons-test-utils": "=0.14.3"
  }
}
