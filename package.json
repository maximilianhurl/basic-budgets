{
  "name": "BasicBudgets",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "test": "npm run eslint . && jest",
    "eslint": "eslint"
  },
  "jest": {
    "scriptPreprocessor": "./jest/scriptPreprocessor",
    "setupEnvScriptFile": "<rootDir>/node_modules/react-native/jestSupport/env.js",
    "testFileExtensions": [
      "es6",
      "js"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "es6"
    ],
    "testPathIgnorePatterns": [
      "node_modules/"
    ],
    "unmockedModulePathPatterns": [
      "promise",
      "source-map",
      "key-mirror",
      "immutable",
      "fetch",
      "react-addons-test-utils",
      "react",
      "fbjs",
      "core-js",
      "babel-polyfill"
    ],
    "verbose": true,
    "preprocessCachingDisabled": true,
    "collectCoverage": true
  },
  "dependencies": {
    "alt": "=0.18.1",
    "alt-container": "https://github.com/maximilianhurl/container/tarball/fix-react-native-container",
    "react-native": "^0.16.0"
  },
  "devDependencies": {
    "babel-cli": "^6.3.15",
    "babel-core": "^6.3.2",
    "babel-jest": "^6.0.1",
    "babel-plugin-syntax-async-functions": "^6.3.13",
    "babel-plugin-transform-regenerator": "^6.3.18",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.0.15",
    "eslint": "=1.5.1",
    "eslint-plugin-react": "=3.5.0",
    "jest-cli": "^0.8.0",
    "react": "^0.14.3",
    "react-addons-test-utils": "^0.14.3"
  }
}
