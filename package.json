{
  "name": "BasicBudgets",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "test": "npm run eslint src/ && jest",
    "report-coverage": "CODECLIMATE_REPO_TOKEN=772fa6cb93e879b4122da58b04edae6f7c2e429a3c38ed07ac088cff7f151e1f codeclimate-test-reporter < coverage/lcov.info",
    "eslint": "eslint"
  },
  "jest": {
    "scriptPreprocessor": "./jest/scriptPreprocessor",
    "setupEnvScriptFile": "./jest/jestEnvironment",
    "testFileExtensions": [
      "es6",
      "js"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "es6"
    ],
    "testPathIgnorePatterns": [
      "node_modules/"
    ],
    "unmockedModulePathPatterns": [
      "promise",
      "source-map",
      "key-mirror",
      "immutable",
      "fetch",
      "react-addons-test-utils",
      "react",
      "fbjs",
      "core-js",
      "babel-polyfill"
    ],
    "verbose": true,
    "preprocessCachingDisabled": true,
    "collectCoverage": true
  },
  "dependencies": {
    "alt": "=0.18.2",
    "alt-container": "=1.0.2",
    "react-native": "=0.20.0"
  },
  "devDependencies": {
    "babel-cli": "=6.4.0",
    "babel-core": "=6.4.0",
    "babel-polyfill": "=6.3.14",
    "babel-preset-es2015": "=6.3.13",
    "babel-preset-react": "=6.3.13",
    "codeclimate-test-reporter": "=0.3.0",
    "eslint": "=1.10.3",
    "eslint-plugin-react": "=3.15.0",
    "fbjs": "^0.6.1",
    "jest-cli": "=0.8.2",
    "react": "=0.14.6",
    "react-addons-test-utils": "=0.14.6"
  }
}
